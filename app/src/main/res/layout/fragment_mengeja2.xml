<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F0E8">

    <ImageView
        android:id="@+id/cloud_top_right"
        android:layout_width="129dp"
        android:layout_height="93dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-9dp"
        android:layout_marginEnd="-11dp"
        android:alpha="0.6"
        android:src="@drawable/desainatas" />

    <ImageView
        android:id="@+id/cloud_right"
        android:layout_width="83dp"
        android:layout_height="156dp"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="0dp"
        android:alpha="0.5"
        android:src="@drawable/desainkanan" />

    <ImageView
        android:id="@+id/cloud_bottom_left"
        android:layout_width="134dp"
        android:layout_height="103dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="0dp"
        android:alpha="0.4"
        android:src="@drawable/desainbawah" />

</RelativeLayout>